from selenium import webdriver  from selenium.webdriver.chrome.service import Service  from selenium.webdriver.chrome.options import Options  from selenium.webdriver.common.by import By  from webdriver_manager.chrome import ChromeDriverManager  import time  options = Options()  options.add_argument('--headless')  options.add_argument('--no-sandbox')  driver = webdriver.Chrome(service=Service(ChromeDriverManager().install()), options=options)  try:      driver.get("https://www.subito.it/annunci-italia/vendita/usato/?q=iphone")      time.sleep(3)      ads = driver.find_elements(By.CLASS_NAME, "items__item")      print(f"SUCCESSO! {len(ads)} annunci")  except Exception as e:      print(f"Errore: {e}")  finally:      driver.quit()  test_quick.py
